I'm a matrix
{% set matrixId = attribute[1] %}
{{ matrixId }}
{% set matrix = anu.matrix.getMatrixByName(matrixId) %}
{% set matrixAttributes =  matrix.defineAttributes() %}
{% for index, attributes in matrixAttributes %}
    <button class="btn btn-default" ng-click="addMatrixElement('{{ index }}', '{{ key }}')" name="{{ index }}" value="{{ index }}">{{ index }}</button>
{% endfor %}


<ul id="{{ class }}{{ entry.id }}{{ key }}" ui-sortable="sortableMatrix" ng-model="matrixElements['{{ key }}']" style="border: solid 1px black"  >
    <li ng-repeat="item in matrixElements['{{ key }}']" style="border: solid 1px black">
        <div style="float: left">
            <i class="fa fa-arrows move" aria-hidden="true"></i>
            <span>
                {[{ item.title }]}
            </span>

            <a href=""  ng-click="removeMatrixElement(item, '{{ key }}')">
                <i class="fa fa-trash-o" aria-hidden="true"></i>
            </a>
        </div>
        <div class="clearfix"></div>
        <div class="panel" ng-repeat="(index, key) in item.attributes">
            <div thin-directive="{[{key[0]}]}" item-info="item"> </div>
        </div>
    </li>
</ul>