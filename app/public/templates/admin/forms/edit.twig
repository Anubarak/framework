{% extends 'admin/layout/site.twig' %}
{% set activePage = 'entries' %}
{% set pageHeadline = (entry)? "Eintrag bearbeiten" : "Neuer Eintrag" %}
{% block content %}
    {% set param = entry? entry : form %}


    {% for answer in entry.lnk_answer %}
        {{ answer.title }}<br>
    {% endfor %}

    {% set form = anu.entry.renderForm(param) %}
    {{ form|raw }}

{% endblock %}

{% includeJsFile 'entryController.js'%}